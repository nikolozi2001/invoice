generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
}

model WaitlistEntry {
  id           String   @id @default(cuid())
  email        String   @unique
  businessName String?
  businessType String?
  createdAt    DateTime @default(now())
  updatedAt    DateTime @updatedAt

  @@map("waitlist_entries")
}